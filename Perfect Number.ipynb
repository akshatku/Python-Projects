    "#perfect number\n",
    "\n",
    "def perfectnum(n):\n",
    "    \n",
    "    sum=0\n",
    "    \n",
    "    for i in range(n):\n",
    "        \n",
    "        if(n%i)==0:\n",
    "            sum+=i\n",
    "    \n",
    "    if sum==n:\n",
    "        x=\"is perfect\"\n",
    "    else:\n",
    "        x=\"is not perfect\"\n",
    "    \n",
    "    return x\n",
    "\n",
